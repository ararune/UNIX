touch a b c d e

12) mkdir zad12
cd zad12
mkdir ~/zad12/Characters && cp ~/zad11/{a,b,c,d,e} ~/zad12/Characters/

13) mkdir zad13
cd zad13
for i in {0..9}; do mkdir $i; done

14) mkdir zad14
cd zad14
date > "My signature"
