3) mkdir zad3
cd zad3
cp -r /materijali/letters ./ && find ./letters -maxdepth 1 -type f -name '??' -delete

4) mkdir zad4
find /var/log -type d -readable -prune -o -type f -name 'syslog.*.gz' -exec basename {} \; > ~/zad4/listing

